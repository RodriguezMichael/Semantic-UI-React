{
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "__DEV__": false,
    "__STAGING__": false,
    "__TEST__": false,
    "__PATH_SEP__": false,
    "__PROD__": false
  },
  "rules": {
    "global-require": 0,
    "jsx-quotes": [2, "prefer-single"],
    "max-len": [2, 120],
    "no-return-assign": [ 2, "except-parens"],
    "no-underscore-dangle": 0,
    "padded-blocks": [2, {
      "blocks": "never",
      "switches": "never",
      "classes": "never"
    }],
    "semi": [2, "never"],
    "jsx-a11y/href-no-hash": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-webpack-loader-syntax": 0,
    "react/jsx-filename-extension": [2, { "extensions": [".js"] }]
  }
}
